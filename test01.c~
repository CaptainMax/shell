#include <dirent.h>
#include <string.h>
#include <time.h>
#include <curses.h>
int fork_1(){
	int childpid;
	int i;
	if(fork()==0){

	for(i = 0; i <= 8; i++){
		printf("This is child process\n");	
	}	
	}else{
	for(i = 1;i <= 8; i++){
		printf("This is parent process\n");
}
}
	printf("step2 after fork()\n\n");
}
